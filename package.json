{
  "name": "koala-ai",
  "version": "2.2.0",
  "main": "dist-electron/main/index.js",
  "description": "TokenAI 的一款AI客户端产品KoalaAI，用于提供AI对话能力。",
  "author": "TokenAI 239573049@qq.com",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:dir": "tsc && vite build && electron-builder --dir",
    "build:win": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ ELECTRON_BUILDER_CACHE=.electron-cache node scripts/clean-build.js && tsc && vite build && electron-builder --win && node scripts/fix-icu.js",
    "build:mac": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ ELECTRON_BUILDER_CACHE=.electron-cache tsc && vite build && electron-builder --mac",
    "build:linux": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ ELECTRON_BUILDER_CACHE=.electron-cache node scripts/clean-build-linux.js && tsc && vite build && electron-builder --linux",
    "build:optimized": "node scripts/build-optimized.js",
    "build:optimized:win": "node scripts/build-optimized.js win",
    "build:optimized:mac": "node scripts/build-optimized.js mac",
    "build:optimized:linux": "node scripts/build-optimized.js linux",
    "clean": "node scripts/clean-build.js",
    "clean:linux": "node scripts/clean-build-linux.js",
    "predownload": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-builder install-app-deps",
    "postinstall": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-builder install-app-deps",
    "optimize": "node scripts/optimize-build.js",
    "fix-builder": "node scripts/fix-app-builder.js",
    "diagnose": "node scripts/diagnose.js",
    "quick-fix": "scripts\\quick-fix.bat",
    "fix-icu": "node scripts/fix-icu.js",
    "preview": "vite preview",
    "pretest": "vite build --mode=test",
    "test": "vitest run",
    "version:current": "node scripts/version-manager.js current",
    "version:info": "node scripts/version-manager.js info",
    "version:bump": "node scripts/version-manager.js bump",
    "version:bump:minor": "node scripts/version-manager.js bump minor",
    "version:bump:major": "node scripts/version-manager.js bump major",
    "version:set": "node scripts/version-manager.js set",
    "release": "node scripts/version-manager.js release",
    "release:push": "node scripts/version-manager.js release --push",
    "release:prerelease": "node scripts/version-manager.js release --prerelease",
    "tag": "node scripts/version-manager.js tag"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@lobehub/icons": "^2.1.0",
    "@lobehub/ui": "^2.1.12",
    "@types/react-syntax-highlighter": "^15.5.13",
    "antd": "^5.26.3",
    "app-builder-bin": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "electron-updater": "^6.3.9",
    "idb": "^8.0.3",
    "katex": "^0.16.22",
    "lucide-react": "^0.511.0",
    "mermaid": "^11.6.0",
    "react-markdown": "^10.1.0",
    "react-merge-refs": "^3.0.2",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.6.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-zoom-pan-pinch": "^3.7.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^7.0.3",
    "electron": "^36.4.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.2",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.1.4"
  }
}
